#Kadane's Algorithm
t = int(input())
for _ in range(t):
    n = int(input())
    inputList = list(map(int, input().split()))
    currMax = result = inputList[0]
    for i in range(1, n):
        currMax = max(inputList[i], currMax + inputList[i])
        if currMax > result:
            result = currMax
    print(result)
